package com.atguigu.java;

public class TestThread {
	public static void main(String[] args) {
		SubThread st1 = new SubThread();
		SubThread st2 = new SubThread();
		st1.start();
		st2.start();
	}
}

class SubThread extends Thread {
	public void run() {
		for (int i = 1; i <= 100; i++) {
			System.out.println(i);
		}
	}
}
